FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    // Coupled solver
    "w|theta"
    {
        tolerance       1e-15;
        relTol          0; //0.1;
        maxIter         1000;

        solver          PBiCGStab;
        preconditioner  DIC;
    }
}

relaxationFactors
{
    equations
    {
    }

    fields
    {
       //  w        0.5;
       // theta        0.7;
    }
}

